import java.util.Scanner;

public class twjorgensen_PunchRun
{
    public static void main ( String args[] )
        {
            Scanner input = new Scanner ( System.in );
            int number1, number2;

            System.out.print ( "Enter first integer: "); // Grabs first number and stores it into number1
            number1 = input.nextInt();

            System.out.print( "Enter second integer: "); // Grabs second number and store it into number2
            number2 = input.nextInt();

            if (number1 == number2)
                System.out.printf( "%d == %d\n", number1, number2 );

            if (number1 != number2)
                System.out.printf( "%d != %d\n", number1, number2);

            if (number1 < number2)
                System.out.printf( "%d < %d\n", number1, number2);

            if (number1 > number2)
                System.out.printf( "%d > %d\n", number1, number2);

            if (number1 <= number2)
                System.out.printf( "%d <= %d\n", number1, number2);

            if (number1 >= number2)
                System.out.printf( "%d >= %d\n", number1, number2);
        }
}

